{
    "name": "dem-serwis",
    "version": "0.0.1",
    "author": "Milesq",
    "license": "ISC",
    "scripts": {
        "dev": "parcel src/*.html --open http://localhost:1234/index.html",
        "build": "parcel build src/index.html src/pages/*.html",
        "test": "jest",
        "lint": "eslint . --fix && postcss -d .tmp src/**/*.scss",
        "clean": "rm -rf .tmp dist .cache"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run test"
        }
    },
    "lint-staged": {
        "(src|tests)/**/*.js": [
            "eslint --fix"
        ],
        "src/**/*.[c(sc)]ss": [
            "postcss -d .tmp"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "autoprefixer": "^9.6.1",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.2",
        "babel-loader": "^8.0.6",
        "babel-polyfill": "^6.26.0",
        "colorguard": "^1.2.1",
        "cssnano": "^4.1.10",
        "doiuse": "^4.2.0",
        "eslint": "^6.2.1",
        "eslint-config-prettier": "^6.1.0",
        "eslint-plugin-prettier": "^3.1.0",
        "husky": "^3.0.4",
        "jest": "^24.9.0",
        "lint-staged": "^9.2.3",
        "node-sass": "^4.12.0",
        "parcel": "^1.12.3",
        "postcss": "^7.0.17",
        "postcss-cli": "^6.1.3",
        "postcss-modules": "^1.4.1",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^1.18.2",
        "stylelint": "^10.1.0",
        "stylelint-config-recommended": "^2.2.0",
        "stylelint-scss": "^3.9.4",
        "typescript": "^3.6.2"
    },
    "dependencies": {
        "navigo": "^7.1.2"
    }
}
